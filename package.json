{
  "name": "@labflow/jupyterlab-extension",
  "version": "1.0.0",
  "description": "LabFlow - AI Development Workflow for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ai",
    "claude",
    "colab",
    "huggingface"
  ],
  "homepage": "https://github.com/labflow/jupyterlab-extension",
  "bugs": {
    "url": "https://github.com/labflow/jupyterlab-extension/issues"
  },
  "license": "MIT",
  "author": {
    "name": "LabFlow Team",
    "email": "team@labflow.dev"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/labflow/jupyterlab-extension.git"
  },
  "scripts": {
